{
  "name": "jaysbot",
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://saranyacp-github.github.io/JaysBot",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "deploy:staging": "npm run build:staging && node scripts/copy-to-staging.js && gh-pages -d dist-staging",
    "deploy:production": "npm run build:production && node scripts/preserve-staging.js && gh-pages -d dist"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "lucide-react": "^0.562.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.9.3",
    "vite": "^4.4.5"
  }
}
